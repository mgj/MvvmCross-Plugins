<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>artm.MvxPlugins.Fetcher</id>
    <title>MvvmCross Fetcher Plugin</title>
    <description>A MvvmCross plugin to to fetch and cache data from e.g. the web</description>
    <version>1.0.0</version>
    <authors>Mikkel Jensen</authors>
    <tags>mvvm mvvmcross xamarin monodroid monotouch wp wpdev windowsphone xamarin wpa81 win81 mvx</tags>
    <dependencies>
      <group targetFramework ="net">
        <dependency id="MvvmCross.Platform" version="4.2.3" />
        <dependency id="Realm" version="0.77.2"/>
        <dependency id="Polly-Signed" version="4.3.0"/>
        <dependency id="modernhttpclient" version="2.4.2"/>
        <dependency id="Fody" version="1.29.4"/>
        <dependency id="DotNetCross.Memory.Unsafe" version="0.2.2"/>
      </group>
    </dependencies>
  </metadata>
  <files>     
    <!-- PCL -->
    <file src="..\Fetcher\artm.MvxPlugins.Fetcher\bin\Release\artm.MvxPlugins.Fetcher.*" target="lib\portable-net45+win+wpa81" />
            
    <!-- droid -->
    <file src="..\Fetcher\artm.MvxPlugins.Fetcher\bin\Release\artm.MvxPlugins.Fetcher.*" target="lib\MonoAndroid10" />
    <file src="BootstrapContent\FetcherPluginBootstrap.cs.pp" 
      target="content\MonoAndroid10\Bootstrap\FetcherPluginBootstrap.cs.pp" />
      
  </files>
</package>