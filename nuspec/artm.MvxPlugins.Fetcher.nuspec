<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>artm.MvxPlugins.Fetcher</id>
    <title>MvvmCross Fetcher Plugin</title>
    <description>A MvvmCross plugin to to fetch and cache data from e.g. the web</description>
    <version>1.0.0</version>
    <authors>Mikkel Jensen</authors>
    <tags>mvvm mvvmcross xamarin monodroid monotouch wp wpdev windowsphone xamarin wpa81 win81 mvx</tags>
    <dependencies>
    <group targetFramework ="net">
      <dependency id="MvvmCross.Platform" version="4.2.3" />
      <dependency id="Newtonsoft.Json" version="9.0.1" />
        <dependency id="Polly-Signed" version="4.3.0"/>
    </group>
    <group targetFramework ="MonoAndroid">
      <dependency id="Newtonsoft.Json" version="9.0.1" />
      <dependency id="MvvmCross.Platform" version="4.2.3" />
        <dependency id="Square.OkHttp" version="2.7.5.0"/>

    </group>
    <group targetFramework ="Xamarin.iOS">
      <dependency id="Newtonsoft.Json" version="9.0.1" />
      <dependency id="MvvmCross.Platform" version="4.2.3" />
    </group>
    <group targetFramework ="portable-net45+win8+wpa81+wp8">
      <dependency id="Newtonsoft.Json" version="9.0.1" />
      <dependency id="MvvmCross.Platform" version="4.2.3" />
        <dependency id="Polly-Signed" version="4.3.0"/>
    </group>
    <group targetFramework ="uap">
      <dependency id="Newtonsoft.Json" version="9.0.1" />
      <dependency id="MvvmCross.Platform" version="4.2.3" />
    </group>
    <group targetFramework ="dotnet">
      <dependency id="Newtonsoft.Json" version="9.0.1" />
      <dependency id="MvvmCross.Platform" version="4.2.3" />
        <dependency id="Polly-Signed" version="4.3.0"/>
    </group>
    </dependencies>
  </metadata>
  <files>     
    <!-- PCL -->
    <file src="..\Fetcher\artm.MvxPlugins.Fetcher\bin\Release\artm.MvxPlugins.Fetcher.*" target="lib\portable-net45+win+wpa81" />

    <!-- DROID -->
    <file src="..\Fetcher\artm.MvxPlugins.Fetcher.Droid\bin\Release\artm.MvxPlugins.Fetcher.*" target="lib\MonoAndroid10" />
    <file src="BootstrapContent\FetcherPluginBootstrapDroid.cs.pp" target="content\MonoAndroid10\Bootstrap\FetcherPluginBootstrap.cs.pp" />

    <!-- TOUCH -->
    <file src="..\Fetcher\artm.MvxPlugins.Fetcher.Touch\bin\Release\artm.MvxPlugins.Fetcher.*" target="lib\Xamarin.iOS10" />
    <file src="BootstrapContent\FetcherPluginBootstrapTouch.cs.pp" target="content\Xamarin.iOS10\Bootstrap\FetcherPluginBootstrap.cs.pp" />

  </files>
</package>